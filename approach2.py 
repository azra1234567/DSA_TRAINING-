
def process_queries(arr, queries):
    n = len(arr)
    
    prefix_sum = [0] * n

    prefix_sum[0] = arr[0]
    for i in range(1, n):
        prefix_sum[i] = prefix_sum[i-1] + arr[i]
        results = []
    
    for L, R in queries:
        if L == 0:
            results.append(prefix_sum[R])
            
        else:
            results.append(prefix_sum[R] - prefix_sum[L-1])
            
    return results
num=int(input("enter your list:"))
num=[]
for i in range(0,N):
a=int(input("enter the {i+1} number:"))
num.append(a)
querysum=[(0,2)(1,3)(4,5),(5)]
print(process_queries(num, querysum))